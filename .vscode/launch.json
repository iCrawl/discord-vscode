{
	"version": "0.5.0",
	"configurations": [
		{
			"name": "Launch Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceRoot}"],
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/dist/**/*.cjs"]
		}
	]
}
